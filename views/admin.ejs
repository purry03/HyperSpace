<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, shrink-to-fit=no"
    />
    <title>Stellar Admin</title>
    <!-- plugins:css -->
    <link
      rel="stylesheet"
      href="/static/vendors/simple-line-icons/css/simple-line-icons.css"
    />
    <link rel="stylesheet" href="/static/vendors/flag-icon-css/css/flag-icon.min.css" />
    <link rel="stylesheet" href="/static/vendors/css/vendor.bundle.base.css" />
    <!-- endinject -->
    <!-- Plugin css for this page -->
    <link
      rel="stylesheet"
      href="/static/vendors/daterangepicker/daterangepicker.css"
    />
    <link rel="stylesheet" href="/static/vendors/chartist/chartist.min.css" />
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <!-- endinject -->
    <!-- Layout styles -->
    <link rel="stylesheet" href="/static/stylesheets/admin.css" />
    <!-- End layout styles -->
    <link rel="shortcut icon" href="/static/img/favicon.ico" />
    <script src="https://kit.fontawesome.com/b1150f83bb.js" crossorigin="anonymous"></script>
  </head>
  <body>
    <div class="container-scroller">
      <!-- partial:partials/_navbar.html -->
      <nav
        class="navbar default-layout-navbar col-lg-12 col-12 p-0 fixed-top d-flex flex-row"
      >

        <div class="navbar-menu-wrapper d-flex align-items-center flex-grow-1">
          <h5 class="mb-0 font-weight-medium d-none d-lg-flex">
            Hyperspace Dashboard
          </h5>
         
          <button
            class="navbar-toggler navbar-toggler-right d-lg-none align-self-center"
            type="button"
            data-toggle="offcanvas"
          >
            <span class="icon-menu"></span>
          </button>
        </div>
      </nav>
      <!-- partial -->
      <div class="container-fluid page-body-wrapper">
        <!-- partial:partials/_sidebar.html -->
        <nav class="sidebar sidebar-offcanvas" id="sidebar">
          <ul class="nav">
            <li class="nav-item nav-profile">
              <a href="#" class="nav-link">
                <div class="text-wrapper">
                  <p class="profile-name">Rohan Sharma</p>
                  <p class="designation">Administrator</p>
                </div>
              </a>
            </li>
           
          </ul>
        </nav>
        <!-- partial -->
        <div class="main-panel">
          <div class="content-wrapper">
            <div
            class="d-md-flex row m-0 quick-action-btns"
            role="group"
            aria-label="Quick action buttons"
          >
            <div class="col-sm-6 col-md-3 p-3 text-center btn-wrapper">
              <button type="button" class="btn px-0">
                <i class="icon-user mr-2"></i> <A href="/admin/upload">Upload Song</A>
              </button>
            </div>
            <div class="col-sm-6 col-md-3 p-3 text-center btn-wrapper">
              <button type="button" class="btn px-0">
                <i class="icon-docs mr-2"></i> Create Quote
              </button>
            </div>
            <div class="col-sm-6 col-md-3 p-3 text-center btn-wrapper">
              <button type="button" class="btn px-0">
                <i class="icon-folder mr-2"></i> Enter Payment
              </button>
            </div>
            <div class="col-sm-6 col-md-3 p-3 text-center btn-wrapper">
              <button type="button" class="btn px-0">
                <i class="icon-book-open mr-2"></i>Create Invoice
              </button>
            </div>
          </div>
          <div class="row">
            <div class="col-md-12 grid-margin">
              <div class="card">
                <div class="card-body">
                  <div class="row">
                    <div class="col-md-12">
                      <div
                        class="d-sm-flex align-items-baseline report-summary-header"
                      >
                        <h5 class="font-weight-semibold">Summary</h5>
                        <span class="ml-auto">Refresh</span>
                        <button class="btn btn-icons border-0 p-2">
                          <i class="icon-refresh"></i>
                        </button>
                      </div>
                    </div>
                  </div>
                  
                  <div class="row report-inner-cards-wrapper">
                    <div class="col-md -6 col-xl report-inner-card">
                      <div class="inner-card-text">
                        <span class="report-title">SONGS</span>
                        <h4 class='song-count'>...</h4>
                        <span class="report-count disk-space">...</span>
                      </div>
                      <div class="inner-card-icon bg-success">
                        <i class="fas fa-music"></i>                      </div>
                    </div>
                    <div class="col-md-6 col-xl report-inner-card">
                      <div class="inner-card-text">
                        <span class="report-title">SONG PLAYS</span>
                        <h4>173</h4>
                        <span class="report-count">Past 24 hours</span>
                      </div>
                      <div class="inner-card-icon bg-primary">
                        <i class="fas fa-play-circle"></i>                      </div>
                    </div>
                    <div class="col-md-6 col-xl report-inner-card">
                      <div class="inner-card-text">
                        <span class="report-title">SEARCH QUERIES</span>
                        <h4>2650</h4>
                        <span class="report-count">Past 12 hours</span>
                      </div>
                      <div class="inner-card-icon bg-warning">
                        <i class="fas fa-search"></i>                      </div>
                    </div>
                    <div class="col-md-6 col-xl report-inner-card">
                      <div class="inner-card-text">
                        <span class="report-title">ERRORS</span>
                        <h4>2</h4>
                        <span class="report-count">Past 7 days</span>
                      </div>
                      <div class="inner-card-icon bg-danger">
                        <i class="fas fa-exclamation-circle"></i>                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- main-panel ends -->
      </div>
      <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="/static/vendors/js/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page -->
    <script src="/static/vendors/chart.js/Chart.min.js"></script>
    <script src="/static/vendors/moment/moment.min.js"></script>
    <script src="/static/vendors/daterangepicker/daterangepicker.js"></script>
    <script src="/static/vendors/chartist/chartist.min.js"></script>
    <!-- End plugin js for this page -->
    <!-- inject:js -->
    <script src="/static/javascripts/off-canvas.js"></script>
    <script src="/static/javascripts/misc.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page -->
    <script src="/static/javascripts/dashboard.js"></script>
    <!-- End custom js for this page -->
  </body>
</html>
